{
  "name": "my-qwik-basic-starter",
  "description": "App with Routing built-in (recommended)",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adaptors/cloudflare-pages/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint src/**/*.ts*",
    "preview": "qwik build preview && vite preview --open",
    "serve": "wrangler pages dev ./dist",
    "serve.express": "node server/entry.express.js",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.13.3",
    "@builder.io/qwik-city": "0.0.122",
    "@builder.io/qwik-react": "0.1.1",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@mui/material": "5.10.14",
    "@mui/x-data-grid": "5.17.12",
    "@types/compression": "^1.7.2",
    "@types/cookie": "^0.5.1",
    "@types/crypto-js": "^4.1.1",
    "@types/eslint": "8.4.10",
    "@types/express": "4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/node": "latest",
    "@types/passport": "^1.0.11",
    "@types/passport-google-oauth": "^1.0.42",
    "@types/qrcode": "^1.5.0",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "autoprefixer": "10.4.13",
    "compression": "^1.7.4",
    "eslint": "8.28.0",
    "eslint-plugin-qwik": "0.13.3",
    "express": "4.18.2",
    "netlify-cli": "^2.37.0",
    "node-fetch": "3.3.0",
    "postcss": "^8.4.19",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.2.4",
    "typescript": "4.9.3",
    "vite": "3.2.4",
    "vite-tsconfig-paths": "3.5.2",
    "wrangler": "latest"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.1.0",
    "barcode-react-scanner": "^1.2.0",
    "cookie": "^0.5.0",
    "crypto-js": "^4.1.1",
    "express-session": "^1.17.3",
    "firebase": "^9.14.0",
    "jimp": "^0.16.2",
    "localtunnel": "^2.0.2",
    "modern-react-qr-reader": "^1.0.12",
    "passport": "^0.6.0",
    "passport-google-oauth": "^2.0.0",
    "qrcode": "^1.5.1",
    "react-barcode-reader": "^0.0.2",
    "react-qr": "^0.0.2",
    "react-qr-barcode-scanner": "^1.0.6",
    "react-qr-code": "^2.0.8",
    "react-qr-reader": "^3.0.0-beta-1",
    "react-qr-scanner": "^1.0.0-alpha.11",
    "roman-numbers": "^1.0.2",
    "tailwind-scrollbar": "^2.0.1",
    "zod": "^3.19.1"
  }
}
